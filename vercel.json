{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/transactions", "dest": "/transactions/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    { "src": "/node_modules/(.*)", "dest": "/node_modules/$1" },
    { "src": "/@id/(.*)", "dest": "/@id/$1" },
    { "src": "/@fs/(.*)", "dest": "/@fs/$1" },
    { "src": "/@vite/(.*)", "dest": "/@vite/$1" },
    { "src": "/@react-refresh", "dest": "/@react-refresh" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
